{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "no-use-before-define": ["error", { "variables": false }],
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "jsx-a11y/accessible-emoji": "off"
  },
  "plugins": ["prettier"]
}
